{
    "name":"hce/laravel-swagger-api",
    "description":"API Swagger package for Laravel.",
    "homepage":"https://gitlab.test5.hce.it/hce/laravel-swagger-api",
    "license":"MIT",
    "authors":[
        {
            "name":"Fabio Savina",
            "email":"fsavina@hce.it"
        }
    ],
    "keywords":[
        "laravel",
        "swagger",
        "api"
    ],
    "require":{
        "illuminate/contracts":"5.5.*",
        "illuminate/http":"5.5.*",
        "illuminate/routing":"5.5.*",
        "illuminate/support":"5.5.*",
        "calcinai/strut":"^0.2.4",
        "swagger-api/swagger-ui":"^3.9"
    },
    "require-dev":{
        "phpunit/phpunit":"^6.5",
        "orchestra/testbench":"^3.5"
    },
    "suggest":{
        "barryvdh/laravel-cors":"Required to send CORS headers (^0.10.0)"
    },
    "autoload":{
        "psr-4":{
            "Finnegan\\Api\\":"src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Finnegan\\Api\\Tests\\":"tests/"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "Finnegan\\Api\\ApiServiceProvider"
            ],
            "aliases":{
                "Api":"Finnegan\\Api\\ApiFacade"
            }
        }
    }
}
